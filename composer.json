    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "openeuropa/code-review": "~0.2",
    "openeuropa/task-runner": "^0.4"
  },
  "autoload": {
    "psr-4": { "SyncopePhpClient\\" : "src/" }
  },
  "autoload-dev": {
    "psr-4": { "SyncopePhpClient\\" : "test/" }
  }
}
