{
    "name": "openeuropa/syncope-php-client",
    "license": "EUPL-1.2",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0|~6.0",
        "openeuropa/code-review": "~0.2",
        "openeuropa/task-runner": "~0.8"
    },
    "autoload": {
        "psr-4": { "OpenEuropa\\SyncopePhpClient\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "OpenEuropa\\SyncopePhpClient\\Test\\" : "test/" }
    }
}
